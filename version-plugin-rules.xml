<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://www.mojohaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
  <ignoreVersions>
    <ignoreVersion type="regex">.*-preview</ignoreVersion>
    <ignoreVersion type="regex">.*[-_\.](alpha|Alpha|ALPHA|b|beta|Beta|BETA|rc|RC|M|EA|pr)[-_\.]?[0-9]*</ignoreVersion>
    <ignoreVersion type="regex">.*jre11</ignoreVersion>
    <ignoreVersion type="regex">.*jre12</ignoreVersion>
    <ignoreVersion type="regex">.*jre14</ignoreVersion>
    <ignoreVersion type="regex">.*jre7</ignoreVersion>
    <ignoreVersion type="regex">.*jre6</ignoreVersion>
  </ignoreVersions>
</ruleset>